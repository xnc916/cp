<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.xiongyuan.lottery.mypage.activity.BetDetailsActivity">
    <include layout="@layout/default_head_bar_back_img"
        android:id="@+id/title"/>
    <ListView
        android:layout_below="@+id/title"
        android:layout_above="@+id/bet_deta_cd"
        android:id="@+id/list_record_number_item"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@null"/>

    <Button
        android:id="@+id/bet_deta_cd"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="@drawable/btn_login"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:textSize="16sp"
        android:layout_alignParentBottom="true"
        android:textColor="@color/colorWhite"
        style="?android:attr/borderlessButtonStyle"
        android:text="撤    单" />

</RelativeLayout>
